<div class="top-center">
  <div class="small-12 medium-8 small-offset-1 medium-offset-2 columns welcome-text">
    <div class="field-man">Full-Stack Web Development with Ruby on Rails</div>
    <div class="row-fluid">
      <div class="small-12 medium-4 columns">
        <div class="panel">
          <h5>Front End</h5>
          <p>
            <%= image_tag "bootstrap-jquery-html5-css3.png" %>
          </p>
        </div>
      </div>
      <div class="small-12 medium-4 columns">
        <div class="panel">
          <h5>Back End</h5>
          <p>
            <div class="row-fluid">
              <div class="small-6 medium-6 columns">
                <%= image_tag "ruby-logo.png" %>
              </div>
              <div class="small-6 medium-6 columns">
                <%= image_tag "rails.png" %>
              </div>
            </div>
            <div class="row-fluid">
              <%= image_tag "postgresql-logo.png" %>
            </div>
          </p>
        </div>
      </div>
      <div class="small-12 medium-4 columns">
        <div class="panel">
          <h5>Tools</h5>
            <div class="row">
              <div class="small-6 medium-6 columns">
                <%= image_tag "github.png" %>
                <div>GitHub</div>
              </div>
              <div class="small-6 medium-6 columns">
                <%= image_tag "heroku.png" %>
                <div>Heroku</div>
              </div>
            </div>
        </div>
      </div>
    </div>
    <div class="row-fluid">
      <div class="small-12 medium-6 medium-offset-3 columns">
        <a href="#" class="small button expand" id="start-application">
          START AN APPLICATION</a>
      </div>
    </div>
  </div>
</div>
